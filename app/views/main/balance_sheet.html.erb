<table>
	<tr>

		<td>
			<p>
				<h1>Liability Accounts</h1>
			</p>
			<div>
				<table>
					<tr>
						<th>Name</th>
						<th>Credit Amount</th>
						<th>Debit Amount</th>
						<th>Account Balance</th>
					</tr>
					<% @liability_accounts.each do |liability_account| %>
					  <tr>
					    <td><%= liability_account.name %></td>
					    <td><%= liability_account.credits_balance %></td>
					    <td><%= liability_account.debits_balance %></td>
					    <td><%= liability_account.balance %></td>
					   
					 
				<% end %>
				 </tr>
					    <tr>
					    <td><B>Final Balance</B></td>
					    <td></td>
					    <td><%= Liability.balance %></td>
					    <td></td>
					   
					  </tr>
				</table>
			</div>
		</td>
		
			<td>
			<p>
				<h1>Asset Accounts</h1>
			</p>
