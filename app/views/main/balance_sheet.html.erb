					    <td><%= asset_account.debits_balance %></td>
					   <td><%= asset_account.balance %></td>
					   
					  </tr>
				<% end %>
				</tr>
					    <tr>
					    <td><B>Final Balance</B></td>
					    <td></td>
					    <td><%= Asset.balance %></td>
					    <td></td>
					   
					  </tr>
				</table>
			</div>
		</td>
		
	</tr>
	<tr>

		
			<td>
			<p>
