<%= form_for(@account) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :type %>
    <%= f.select :type, options_for_select(@account_types) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
